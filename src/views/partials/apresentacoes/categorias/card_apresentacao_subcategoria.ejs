<div class="container">
    <br>
    <br>
    <div id="categorias" class="row view-group">
        <% subcategorias.forEach(element => { %>
        <div class="item col-xs-4 col-lg-4">
            <div class="thumbnail card">
                <div class="img-event">
                    <img class="group list-group-image img-fluid img-produto-card-pequeno"
                        src="<%= element.foto %>" alt="img" />
                </div>
                <div class="caption card-body">
                    <h4 class="group card-title inner list-group-item-heading">
                        <%= element.nome %></h4>
                    <p class="group inner list-group-item-text">
                        <%= element.descricao %></p>
                    <div class="row">
                        <div class="col-xs-12 col-md-6">
                            <p class="lead"><%= element.likes%> Likes</p>
                        </div>
                        <div class="col-xs-12 col-md-6">
                            <a class="btn btn-success" href="/categorias/subcategoria/<%= element.id %>">Saber Mais</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <% }) %>
    </div>
</div>